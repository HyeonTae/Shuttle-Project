<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/gosmu"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="smu.shuttleapp.search.all.GoSMUActivity" >

    <Button
        android:id="@+id/fromAsanBut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="280dp"
        android:onClick="fromAsan"
        android:background="@drawable/fromasan" />

    <Button
        android:id="@+id/fromCheonAnBut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="fromCheonAn"
        android:background="@drawable/fromcheonan" />

    <Button
        android:id="@+id/fromTerminalBut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="fromTerminal"
        android:background="@drawable/fromterminal" />

</LinearLayout>
