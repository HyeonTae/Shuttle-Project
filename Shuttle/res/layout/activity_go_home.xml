<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/gohome"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="smu.shuttleapp.home.GoHomeActivity" >

    <Button
        android:id="@+id/forAsanBut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="280dp"
        android:onClick="forAsan"
        android:background="@drawable/toasan" />

    <Button
        android:id="@+id/forCheonAnBut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="forCheonAn"
        android:background="@drawable/tocheonan" />

    <Button
        android:id="@+id/forTerminalBut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="forTerminal"
        android:background="@drawable/toterminal" />

</LinearLayout>
